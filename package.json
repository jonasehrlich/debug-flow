{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --build && vite build",
    "build:analyze": "tsc --build && ANALYZE_ROLLUP=true vite build",
    "typecheck": "tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier --write .",
    "fmt:files": "prettier --write",
    "fmt:check": "prettier --check .",
    "api:create": "cargo run --bin dump-openapi-schema && npx openapi-typescript openapi-schema.json -o frontend/src/types/api.ts",
    "api:check": "cargo run --bin dump-openapi-schema && npx openapi-typescript openapi-schema.json --check -o frontend/src/types/api.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "cargo run --bin dump-openapi-schema && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.10",
    "@xyflow/react": "^12.8.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.523.0",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.14.0",
    "react": "^19.1.0",
    "react-diff-view": "^3.3.2",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-markdown": "^10.1.0",
    "react-remove-scroll": "^2.7.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "ts-key-enum": "^3.0.13",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.6.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitepress": "^2.0.0-alpha.8",
    "vitepress-openapi": "^0.1.4",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.0"
  },
  "overrides": {
    "vitepress-openapi": {
      "vitepress": "2.0.0-alpha.8"
    }
  }
}
